#include<iostream>
using namespace std;

class Employee
{
    private:
        int a,b,c;
    public:
        int d,e;
        void setdata(int a1,int b2,int c3);  //Decleration
        void main(){
            cout<<"The value of a is "<<a<<endl;
            cout<<"The value of b is "<<b<<endl;
            cout<<"The value of c is "<<c<<endl;
            cout<<"The value of d is "<<d<<endl;
            cout<<"The value of e is "<<e<<endl;
        }
};

    void Employee :: setdata(int a1,int b2,int c3){
        a=a1;
        b=b2;
        c=c3;
    }

int main(){
    Employee Bharat;
    //Bharat.a = 122;  <<< This will show error as a is private
    Bharat.setdata(1,2,4);
    Bharat.d=22;
    Bharat.e=33;
    Bharat.main();
    return 0;
}
